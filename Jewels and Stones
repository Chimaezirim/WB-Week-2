class Solution {
    public int numJewelsInStones(String J, String S) {
        char j, s;
        int count = 0;
        for(int i = 0; i < J.length(); i++){
            j = J.charAt(i);
            for(int z = 0; z < S.length(); z++){
                if(j == S.charAt(z))
                    count++;
            }
        }
        return count;
    }
}
